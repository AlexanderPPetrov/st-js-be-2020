    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.7",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.10",
    "uuid": "^3.3.2",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "10.3.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
