    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-dom": "^2.1.1",
    "nodemon": "^1.18.4",
    "react-testing-library": "^5.2.3"
  },
  "dependencies": {
    "babel-upgrade": "^1.0.0",
    "bcrypt": "^3.0.8",
